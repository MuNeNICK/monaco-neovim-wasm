{
  "name": "@monaco-neovim-wasm/lib",
  "version": "0.1.9",
  "private": false,
  "type": "module",
  "description": "Core library for running Neovim (WASM) behind Monaco via msgpack-RPC.",
  "scripts": {
    "build": "vite build && vite build -c vite.worker.nvim.config.ts && vite build -c vite.worker.asyncify.config.ts && npm run build:types",
    "typecheck": "tsc --noEmit",
    "build:types": "tsc -p tsconfig.build.json"
  },
  "dependencies": {
    "@bjorn3/browser_wasi_shim": "^0.4.2",
    "fflate": "^0.8.2"
  },
  "peerDependencies": {
    "monaco-editor": "^0.44.0"
  },
  "main": "./dist/monaco-neovim-wasm.es.js",
  "module": "./dist/monaco-neovim-wasm.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/monaco-neovim-wasm.es.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "package.json"
  ]
}
